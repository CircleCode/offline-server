<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bindings>
<bindings id="document-[FAMNAME]-Bindings"
    xmlns="http://www.mozilla.org/xbl"
    xmlns:xul="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
    xmlns:xbl="http://www.mozilla.org/xbl">
    
    <binding id="document-[FAMNAME]-view">
        
        <content>
            <xul:vbox flex="1">
                <dcpDocument xbl:inherits="initid" mode="view" fromname="[FAMNAME]" fromid="[FAMID]">
                <dcpDocMenu/>
                <dcpDocHeader/>
<!-- Begin generated content -->
[viewContent]
<!-- End generated content -->
                <dcpDocFooter/>
                </dcpDocument>
            </xul:vbox>
        </content>
    </binding>
    
    <binding id="document-[FAMNAME]-edit">
        
        <content>
            <xul:vbox flex="1">
                <dcpDocument xbl:inherits="initid" mode="edit" fromname="[FAMNAME]" fromid="[FAMID]">
                <dcpDocMenu/>
                <dcpDocHeader/>
<!-- Begin generated content -->
[editContent]
<!-- End generated content -->
                <dcpDocFooter/>
                </dcpDocument>
            </xul:vbox>
        </content>
        
    </binding>
    
</bindings>
